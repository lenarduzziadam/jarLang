forge commune (a, b) {
  mend a + b
}
forge banish (a, b) {
  mend a - b
}
forge rally (a, b) {
  mend a * b
}
forge slash (a, b){
  mend a / b
}
forge ascend (a, b){
  mend a ^ b
}
forge circle_area (r){
  mend pi * r^2
}
